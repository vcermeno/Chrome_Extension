{
    "manifest_version": 2,
    "name": "Delete My History",
    "version": "0.1.0",
    "description": "When you want no one to know what you were up to ;)",
    "browser_action": {
      "default_icon": "icon.png"
    },
    "background": {
      "scripts":["script.js"]
    },
    "permissions": [
      "browsingData"
    ]
  }
  